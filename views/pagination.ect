<% if @control?.length : %>
	<div class="pagination offscreen">
		<div class="trigger">toggle</div>
		<% for slide in @options.slides : %>
			<img src="<%= slide.thumb %>" />
		<% end %>
	</div>
<% else : %>
	<ul class="progress_indicator">
		<% slides = 1;%>
		<% for slide in @options.slides : %>
			<li>slide <%- slides++%></li>
		<% end %>
	</ul>
<% end %>